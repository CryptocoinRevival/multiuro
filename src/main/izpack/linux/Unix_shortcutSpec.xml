<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="MultiUro"
  location="applications"/>

   <shortcut
     name="MultiUro"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/multiuro-exe.jar"
     initialState="normal"
     iconFile="$INSTALL_PATH/multiuro48.png"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="MultiUro">
   </shortcut>
   <shortcut
     name="Uninstall MultiUro"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/Uninstaller/uninstaller.jar"
     initialState="normal"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="Uninstall MultiUro">
   </shortcut>
</shortcuts>

